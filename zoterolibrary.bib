@article{Matsumoto2016,
  title = {Potential Contributions of Forestry and Wood Use to Climate Change Mitigation in {{Japan}}},
  author = {Matsumoto, Mitsuo and Oka, Hiroyasu and Mitsuda, Yasushi and Hashimoto, Shoji and Kayo, Chihiro and Tsunetsugu, Yuko and Tonosaki, Mario},
  year = {2016},
