@article{Baskent2005,
  title = {Spatial Forest Planning: {{A}} Review},
  author = {Baskent, Emin Zeki and Keles, Sedat},
  year = {2005},
  journal = {Ecological Modelling},
  volume = {188},
  number = {2-4},
