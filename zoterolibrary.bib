@article{Rechid2009,
  title = {Parameterization of Snow-Free Land Surface Albedo as a Function of Vegetation Phenology Based on {{MODIS}} Data and Applied in Climate Modelling},
  author = {Rechid, Diana and Raddatz, Thomas J. and Jacob, Daniela},
  year = {2009},
