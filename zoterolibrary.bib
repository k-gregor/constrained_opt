@article{Proe2002,
  title = {Effects of Spacing, Species and Coppicing on Leaf Area, Light Interception and Photosynthesis in Short Rotation Forestry},
  author = {Proe, M. F. and Griffiths, J. H. and Craig, J.},
  year = {2002},
  journal = {Biomass and Bioenergy},
