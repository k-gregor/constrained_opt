  author = {Hua, Fangyuan and Bruijnzeel, L. Adrian and Meli, Paula and Martin, Philip A. and Zhang, Jun and Nakagawa, Shinichi and Miao, Xinran and Wang, Weiyi and McEvoy, Christopher and {Pe{\~n}a-Arancibia}, Jorge Luis and Brancalion, Pedro H. S. and Smith, Pete and Edwards, David P. and Balmford, Andrew},
  year = {2022},
  month = may,
  journal = {Science},
  volume = {376},
  number = {6595},
  pages = {839--844},
  issn = {0036-8075},
  doi = {10.1126/science.abl4649},
  abstract = {Forest restoration is being scaled up globally to deliver critical ecosystem services and biodiversity benefits; however, there is a lack of rigorous comparison of cobenefit delivery across different restoration approaches. Through global synthesis, we used 25,950 matched data pairs from 264 studies in 53 countries to assess how delivery of climate, soil, water, and wood production services, in addition to biodiversity, compares across a range of tree plantations and native forests. Benefits of aboveground carbon storage, water provisioning, and especially soil erosion control and biodiversity are better delivered by native forests, with compositionally simpler, younger plantations in drier regions performing particularly poorly. However, plantations exhibit an advantage in wood production. These results underscore important trade-offs among environmental and production goals that policy-makers must navigate in meeting forest restoration commitments.},
  file = {/home/konni/Zotero/storage/8ES68F68/science.abl4649.pdf}
}

