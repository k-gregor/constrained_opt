@article{Peters2017,
  title = {Key Indicators to Track Current Progress and Future Ambition of the {{Paris Agreement}}},
  author = {Peters, Glen P. and Andrew, Robbie M. and Canadell, Josep G. and Fuss, Sabine and Jackson, Robert B. and Korsbakken, Jan Ivar and Le Qu{\'e}r{\'e}, Corinne and Nakicenovic, Nebojsa},
  year = {2017},
  journal = {Nature Climate Change},
  volume = {7},
  number = {2},
  pages = {118--122},
  issn = {17586798},
  doi = {10.1038/nclimate3202},
  abstract = {Current emission pledges to the Paris Agreement appear insufficient to hold the global average temperature increase to well below 2 {$^\circ$}C above pre-industrial levels. Yet, details are missing on how to track progress towards the {\^a} {\texteuro} Paris goal', inform the five-yearly {\^a} {\texteuro} global stocktake', and increase the ambition of Nationally Determined Contributions (NDCs). We develop a nested structure of key indicators to track progress through time. Global emissions track aggregated progress, country-level decompositions track emerging trends that link directly to NDCs, and technology diffusion indicates future reductions. We find the recent slowdown in global emissions growth is due to reduced growth in coal use since 2011, primarily in China and secondarily in the United States. The slowdown is projected to continue in 2016, with global CO 2 emissions from fossil fuels and industry similar to the 2015 level of 36 GtCO 2. Explosive and policy-driven growth in wind and solar has contributed to the global emissions slowdown, but has been less important than economic factors and energy efficiency. We show that many key indicators are currently broadly consistent with emission scenarios that keep temperatures below 2 {$^\circ$}C, but the continued lack of large-scale carbon capture and storage threatens 2030 targets and the longer-term Paris ambition of net-zero emissions.},
  file = {/home/konni/Zotero/storage/IKP47L3Y/nclimate3202.pdf}
}

@article{Peters2018,
  title = {Increased Water-Use Efficiency and Reduced {{CO2}} Uptake by Plants during Droughts at a Continental Scale},
  author = {Peters, Wouter and {van der Velde}, Ivar R. and {van Schaik}, Erik and Miller, John B. and Ciais, Philippe and Duarte, Henrique F. and {van der Laan-Luijkx}, Ingrid T. and {van der Molen}, Michiel K. and Scholze, Marko and Schaefer, Kevin and Vidale, Pier Luigi and Verhoef, Anne and W{\aa}rlind, David and Zhu, Dan and Tans, Pieter P. and Vaughn, Bruce and White, James W.C.},
