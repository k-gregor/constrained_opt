  author = {Sardans, Jordi and Vallicrosa, Helena and Zuccarini, Paolo and {Farr{\'e}-Armengol}, Gerard and {Fern{\'a}ndez-Mart{\'i}nez}, Marcos and Peguero, Guille and {Gargallo-Garriga}, Albert and Ciais, Philippe and Janssens, Ivan A. and Obersteiner, Michael and Richter, Andreas and Pe{\~n}uelas, Josep},
