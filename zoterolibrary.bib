@article{Blattert2023,
  title = {Climate Targets in {{European}} Timber-Producing Countries Conflict with Goals on Forest Ecosystem Services and Biodiversity},
  author = {Blattert, Clemens and M{\"o}nkk{\"o}nen, Mikko and Burgas, Daniel and Di Fulvio, Fulvio and Tora{\~n}o Caicoya, Astor and Vergarechea, Marta and Klein, Julian and Hartikainen, Markus and {Ant{\'o}n-Fern{\'a}ndez}, Clara and Astrup, Rasmus and Emmerich, Michael and Forsell, Nicklas and Lukkarinen, Jani and Lundstr{\"o}m, Johanna and Pitz{\'e}n, Samuli and Poschenrieder, Werner and Primmer, Eeva and Sn{\"a}ll, Tord and Eyvindson, Kyle},
