@article{Schelhaas2015,
  title = {Alternative Forest Management Strategies to Account for Climate Change-Induced Productivity and Species Suitability Changes in {{Europe}}},
  author = {Schelhaas, Mart Jan and Nabuurs, Gert Jan and Hengeveld, Geerten and Reyer, Christopher and Hanewinkel, Marc and Zimmermann, Niklaus E. and Cullmann, Dominik},
