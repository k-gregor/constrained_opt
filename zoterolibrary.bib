@article{Zhao2022,
  title = {A Review of Forest Carbon Cycle Models on Spatiotemporal Scales},
  author = {Zhao, Junfang and Liu, Dongsheng and Zhu, Yujie and Peng, Huiwen and Xie, Hongfei},
