@article{Schelhaas2003,
  title = {Natural Disturbances in the {{European}} Forests in the 19th and 20th Centuries},
  author = {Schelhaas, Mart Jan and Nabuurs, Gert Jan and Schuck, Andreas},
  year = {2003},
  journal = {Global Change Biology},
  volume = {9},
