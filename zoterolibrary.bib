@article{Reyer2014,
  title = {Projections of Regional Changes in Forest Net Primary Productivity for Different Tree Species in {{Europe}} Driven by Climate Change and Carbon Dioxide},
  author = {Reyer, Christopher and {Lasch-Born}, Petra and Suckow, Felicitas and Gutsch, Martin and Murawski, Aline and Pilz, Tobias},
  year = {2014},
  journal = {Annals of Forest Science},
  volume = {71},
  number = {2},
  pages = {211--225},
  issn = {12864560},
  doi = {10.1007/s13595-013-0306-8},
  abstract = {{$\bullet$} Context: Projecting changes in forest productivity in Europe is crucial for adapting forest management to changing environmental conditions. {$\bullet$} Aims: The objective of this paper is to project forest productivity changes under different climate change scenarios at a large number of sites in Europe with a stand-scale process-based model. {$\bullet$} Methods: We applied the process-based forest growth model 4C at 132 typical forest sites of important European tree species in ten environmental zones using climate change scenarios from three different climate models and two different assumptions about CO 2 effects on productivity. {$\bullet$} Results: This paper shows that future forest productivity will be affected by climate change and that these effects depend strongly on the climate scenario used and the persistence of CO2 effects. We find that productivity increases in Northern Europe, increases or decreases in Central Europe, and decreases in Southern Europe. This geographical pattern is mirrored by the responses of the individual tree species. The productivity of Scots pine and Norway spruce, mostly located in central and northern Europe, increases while the productivity of Common beech and oak in southern regions decreases. It is important to note that we consider the physiological response to climate change excluding disturbances or management. {$\bullet$} Conclusions: Different climate change scenarios and assumptions about the persistence of CO2 effects lead to uncertain projections of future forest productivity. These uncertainties need to be integrated into forest management planning and adaptation of forest management to climate change using adaptive management frameworks. {\copyright} 2013 INRA and Springer-Verlag France.},
  keywords = {4C (FORESEE),CO2 effects,Environmental change,Level-II plots,Process-based modelling,Uncertainties},
  file = {/home/konni/Zotero/storage/QK6A3BET/13595_2013_Article_306.pdf}
}

@article{Reyer2017,
  title = {Are Forest Disturbances Amplifying or Canceling out Climate Change-Induced Productivity Changes in {{European}} Forests?},
  author = {Reyer, Christopher P.O. and Bathgate, Stephen and Blennow, Kristina and Borges, Jose G. and Bugmann, Harald and Delzon, Sylvain and Faias, Sonia P. and {Garcia-Gonzalo}, Jordi and Gardiner, Barry and {Gonzalez-Olabarria}, Jose Ramon and Gracia, Carlos and Hern{\'a}ndez, Juan Guerra and Kellom{\"a}ki, Seppo and Kramer, Koen and Lexer, Manfred J. and Lindner, Marcus and Van Der Maaten, Ernst and Maroschek, Michael and Muys, Bart and Nicoll, Bruce and Palahi, Marc and Palma, Jo{\~a}o H.N. and Paulo, Joana A. and Peltola, Heli and Pukkala, Timo and Rammer, Werner and Ray, Duncan and Sabat{\'e}, Santiago and Schelhaas, Mart Jan and Seidl, Rupert and Temperli, Christian and Tom{\'e}, Margarida and Yousefpour, Rasoul and Zimmermann, Niklaus E. and Hanewinkel, Marc},
  year = {2017},
  journal = {Environmental Research Letters},
  volume = {12},
  number = {3},
  issn = {17489326},
  doi = {10.1088/1748-9326/aa5ef1},
  abstract = {Recent studies projecting future climate change impacts on forests mainly consider either the effects of climate change on productivity or on disturbances. However, productivity and disturbances are intrinsically linked because 1) disturbances directly affect forest productivity (e.g. via a reduction in leaf area, growing stock or resource-use efficiency), and 2) disturbance susceptibility is often coupled to a certain development phase of the forest with productivity determining the time a forest is in this specific phase of susceptibility. The objective of this paper is to provide an overview of forest productivity changes in different forest regions in Europe under climate change, and partition these changes into effects induced by climate change alone and by climate change and disturbances. We present projections of climate change impacts on forest productivity from state-of-The-Art forest models that dynamically simulate forest productivity and the effects of the main European disturbance agents (fire, storm, insects), driven by the same climate scenario in seven forest case studies along a large climatic gradient throughout Europe. Our study shows that, in most cases, including disturbances in the simulations exaggerate ongoing productivity declines or cancel out productivity gains in response to climate change. In fewer cases, disturbances also increase productivity or buffer climate-change induced productivity losses, e.g. because low severity fires can alleviate resource competition and increase fertilization. Even though our results cannot simply be extrapolated to other types of forests and disturbances, we argue that it is necessary to interpret climate change-induced productivity and disturbance changes jointly to capture the full range of climate change impacts on forests and to plan adaptation measures.},
  keywords = {Fire,Forest models,Forest productivity-disturbances-climate change in,Insects,Storms,Trade-offs},
  file = {/home/konni/Zotero/storage/JMCIG6GA/Reyer_2017_Environ._Res._Lett._12_034027.pdf}
}

