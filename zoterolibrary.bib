@article{Wirth2023,
  title = {Connecting {{CSR}} Theory and {{LPJmL}} 5.3 to Assess the Role of Environmental Conditions, Management and Functional Diversity for Grassland Ecosystem Functions},
  author = {Wirth, Stephen Bj{\"o}rn and Poyda, Arne and Taube, Friedhelm and Tietjen, Britta and M{\"u}ller, Christoph and Thonicke, Kirsten and Linst{\"a}dter, Anja and Behn, Kai and Rolinski, Susanne},
