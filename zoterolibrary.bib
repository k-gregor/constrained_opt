  author = {Sommerfeld, Andreas and Senf, Cornelius and Buma, Brian and D'Amato, Anthony W. and Despr{\'e}s, Tiphaine and {D{\'i}az-Hormaz{\'a}bal}, Ignacio and Fraver, Shawn and Frelich, Lee E. and Guti{\'e}rrez, {\'A}lvaro G. and Hart, Sarah J. and Harvey, Brian J. and He, Hong S. and Hl{\'a}sny, Tom{\'a}{\v s} and Holz, Andr{\'e}s and Kitzberger, Thomas and Kulakowski, Dominik and Lindenmayer, David and Mori, Akira S. and M{\"u}ller, J{\"o}rg and Paritsis, Juan and Perry, George L. W. and Stephens, Scott L. and Svoboda, Miroslav and Turner, Monica G. and Veblen, Thomas T. and Seidl, Rupert},
