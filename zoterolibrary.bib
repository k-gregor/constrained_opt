@article{Klein2014,
  title = {The Variability of Stomatal Sensitivity to Leaf Water Potential across Tree Species Indicates a Continuum between Isohydric and Anisohydric Behaviours},
  author = {Klein, Tamir},
  year = {2014},
  journal = {Functional Ecology},
  volume = {28},
