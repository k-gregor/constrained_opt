@article{Fidej2018,
  title = {Drivers of Regeneration Dynamics Following Salvage Logging and Different Silvicultural Treatments in Windthrow Areas in {{Slovenia}}},
  author = {Fidej, Gal and Rozman, Andrej and Diaci, Jurij},
  year = {2018},
