@article{Pereira2012,
  title = {Global Biodiversity Change: {{The Bad}}, the Good, and the Unknown},
  author = {Pereira, Henrique Miguel and Navarro, Laetitia Marie and Martins, In{\^e}s Santos},
  year = {2012},
  journal = {Annual Review of Environment and Resources},
  volume = {37},
